{
  "name": "unzer/unzer-core",
  "description": "Unzer Integration Core Library",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=7.4",
    "unzerdev/php-sdk": "^3.11.0",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Unzer\\Core\\Infrastructure\\": "src/Infrastructure",
      "Unzer\\Core\\BusinessLogic\\": "src/BusinessLogic"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Unzer\\Core\\Tests\\Infrastructure\\": "tests/Infrastructure",
      "Unzer\\Core\\Tests\\BusinessLogic\\": "tests/BusinessLogic"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.14"
  }
}
